					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"The Expected Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"const items = pm.response.json();",
									"pm.test(\"The Expected Response is an array \", function () {",
									"    pm.expect(items).to.be.an('array');",
									"});",
									"pm.test(\"The expected no. of items is 60 (The actual is \"+ items.length+')', () => {",
									"    pm.expect(items.length).to.eq(60);",
									"    pm.expect(items).to.be.an(\"array\");",
									"});",
									"",
									"pm.test(\"Test: The expected sale-item attributes\", function () {",
									"     items.forEach((item, index) => {",
									"            pm.expect(Object.keys(item).length).to.greaterThanOrEqual(7);",
									"            pm.expect(item).to.have.property('id');",
									"            pm.expect(item).to.have.property('brandName');",
									"            pm.expect(item).to.have.property('model');",
									"            pm.expect(item).to.have.property('ramGb');",
									"            pm.expect(item).to.have.property('storageGb');",
									"     });",
									"});",
									"",
									"",
									"nullColorId = 0;",
									"nullRamId = 0;",
									"nullStorageId = 0;",
									"nullScreenSizeId = 0;",
									"items.forEach((item, index) => {",
									"    if(item.color==null) {",
									"        nullColorId = item.id;",
									"    }",
									"    if(item.ramGb==null) {",
									"        nullRamId = item.id;",
									"    }",
									"    if(item.storageGb==null) {",
									"        nullStorageId = item.id;",
									"    }",
									"    if(item.screenSizeInch==null) {",
									"        nullScreenSizeId = item.id;",
									"    }",
									"});",
									"pm.test(\"Test: The expected some field of any item must be null\", function () {",
									"    pm.expect(nullColorId).to.not.equals(0);",
									"    pm.expect(nullRamId).to.not.equals(0);",
									"    pm.expect(nullStorageId).to.not.equals(0);",
									"    pm.expect(nullScreenSizeId).to.not.equals(0);",
									"});",
									"",
									"pm.test(\"Test: Sale item ordering \", function () {",
									"    id = 0;",
									"    items.forEach((item, index) => {",
									"        pm.expect(item.id).to.gt(id);",
									"        id = item.id;",
									"    });",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
